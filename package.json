    "@types/mdx-js__react": "^1.5.4",
    "@types/react-tabs": "2.3.3",
    "babel-preset-react-app": "^10.0.0",
    "classnames": "^2.2.6",
    "color": "^4.0.1",
    "color-string": "^1.6.0",
    "dompurify": "2.3.1",
    "dotenv": "^10.0.0",
    "gatsby": "^3.11.1",
    "gatsby-plugin-canonical-urls": "^3.11.0",
    "gatsby-plugin-catch-links": "^3.11.0",
    "gatsby-plugin-dark-mode": "^1.1.2",
