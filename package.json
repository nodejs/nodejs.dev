    "intersection-observer": "^0.12.0",
    "node-fetch": "^2.6.1",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.0.0",
    "react-icons": "^4.2.0",
    "react-tabs": "3.2.2",
    "throttle-debounce": "^3.0.1",
    "typescript": "4.3.5"
  },
  "keywords": [
    "gatsby",
    "node.js"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-ci": "gatsby build --prefix-paths",
    "start": "gatsby develop",
    "format": "prettier --write \"**/*.{ts,tsx,js,scss}\" && npm run lint:fix",
    "format-check": "prettier --check \"**/*.{ts,tsx,js,scss}\"",
    "update-snapshot": "npm run test -- --updateSnapshot",
    "pretest": "npm run format-check && npm run lint",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-ci": "npm run test -- --coverage",
    "lint": "npm run type-check && npm run lint:js && npm run lint:md && npm run lint:scss",
    "lint:js": "eslint .",
    "lint:md": "remark -qf .",
    "lint:scss": "stylelint --config ./.stylelintrc.json ./src/**/*.{css,sass,scss}",
    "lint:fix": "npm run lint -- --fix",
    "serve": "npm run build && clear && gatsby serve",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./public/storybook",
    "a11y": "./node_modules/pa11y/bin/pa11y.js localhost:8000",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/semver": "^7.3.2",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/classnames": "^2.3.1",
    "@types/color": "^3.0.2",
    "@types/color-string": "^1.5.0",
    "@types/dompurify": "^2.2.3",
    "@types/jest": "^27.0.1",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "6.1.2",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-remove-graphql-queries": "^3.11.0",
    "babel-preset-gatsby": "1.12.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "^4.10.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "node-sass": "6.0.1",
    "pa11y": "6.0.1",
    "prettier": "^2.3.2",
    "remark-cli": "^10.0.0",
    "remark-frontmatter": "^4.0.0",
    "remark-preset-lint-node": "^3.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodejs/nodejs.dev"
  },
  "resolutions": {
    "sharp": "0.27.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx|js)$": "<rootDir>/test-preprocessor.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby|gatsby-plugin-mdx)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-test-env.js"
    ],
    "collectCoverageFrom": [
      "src/**/**.(ts|tsx|js)",
      "util-node/**/**.(ts|tsx|js)"
    ],
    "testEnvironment": "jsdom"
  }
}
