{
  "hosting": {
    "target": "nodejs-dev",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "headers": [
    {
      "source": "**/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/get-involved/",
      "destination": "/community/",
      "type": "301"
    },
    {
      "source": "/governance/",
      "destination": "/about/governance/",
      "type": "301"
    },
    {
      "source": "/working-groups/",
      "destination": "/about/working-groups/",
      "type": "301"
    },
    {
      "source": "/releases/",
      "destination": "/about/releases/",
      "type": "301"
    },
    {
      "source": "/privacy/",
      "destination": "/about/privacy/",
      "type": "301"
    },
    {
      "source": "/security/",
      "destination": "/about/security/",
      "type": "301"
    },
    {
      "source": "/trademark/",
      "destination": "https://trademark-policy.openjsf.org/",
      "type": "301"
    },
    {
      "source": "/about/trademark/",
      "destination": "https://trademark-policy.openjsf.org/",
      "type": "301"
    }
  ]
}